{
  "name": "@jupiterone/graph-sentinelone",
  "version": "1.1.1",
  "description": "A JupiterOne managed integration for https://www.sentinelone.com/",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "jupiterone"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "graph:spec": "j1-integration visualize-types --project-path docs/spec --output-file ./.j1-integration/types-graph/index.spec.html",
    "validate:questions:dry": "j1-integration validate-question-file --dry-run",
    "validate:questions": "j1-integration validate-question-file -a $MANAGED_QUESTIONS_JUPITERONE_ACCOUNT_ID -k $MANAGED_QUESTIONS_JUPITERONE_API_KEY",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepush": "yarn lint && yarn type-check && jest --changedSince main",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^7.4.0"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-core": "^7.4.0",
    "@jupiterone/integration-sdk-dev-tools": "^7.4.0",
    "@jupiterone/integration-sdk-testing": "^7.4.0",
    "@types/node-fetch": "^2.5.10"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
